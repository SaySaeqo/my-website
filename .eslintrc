{
    "root": true,
    "env": {
        "node": true,
        "es2022": true
    },
    "extends": [
        "plugin:vue/vue3-essential",
        "@vue/typescript/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}